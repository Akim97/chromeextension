{
  "manifest_version": 3,
  "name": "Image Replacer",
  "description": "Changes Images",
  "version": "1.0",
  "icons": {
    "16": "Images/IMG_1050.png",
    "32": "Images/IMG_1050.png",
    "48": "Images/IMG_1050.png",
    "128": "Images/IMG_1050.png"
  },
  "content_scripts": [
    {
      "js": ["script.js"],
      "matches": ["<all_urls>"],
      "web_accessible_resources": [
        {
          "resources": ["/images/*"],
          // "resources": ["IMG_0463.png", "IMG_0503.png", "IMG_1050.png", "IMG_8493.png", "IMG_8652.png", "IMG_8669.png", "IMG_8980.png", "IMG_9018.png", "IMG_9098.png"],
          "matches": ["<all_urls>"]
        }
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["tabs", "unlimitedStorage"],
  "host_permissions": ["https://*/*", "http://*/*"]
}
